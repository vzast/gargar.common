{
  "ConnectionStrings": {
    "DefaultConnection": "Host=yourserver;Port;Database=CommonDb;Username=postgres;Password=password"
  },

  "IdentityOptions": {
    "Password": {
      "RequireDigit": true,
      "RequiredLength": 8,
      "RequireNonAlphanumeric": false,
      "RequireUppercase": true,
      "RequireLowercase": true
    },
    "SignIn": {
      "RequireConfirmedEmail": false,
      "RequireConfirmedPhoneNumber": false,
      "RequireConfirmedAccount": false
    },
    "Tokens": {
      "EmailConfirmationTokenProvider": "email",
      "AuthenticatorTokenProvider": "authenticator"
    },
    "User": {
      "RequireUniqueEmail": true,
      "AllowedUserNameCharacters": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789-._@+"
    },
    "Lockout": {
      "MaxFailedAccessAttempts": 5
    }
  },

  "StripeOptions": {
    "SecretKey": "your_stripe_secret_key",
    "PublishableKey": "your_stripe_publishable_key",
    "FrontendUrl": "https://your-frontend-url.com",
    "WebhookKey": "your_webhook_key"
  },

  "EmailOptions": {
    "Host": "your.smtp.server",
    "Port": 587,
    "Username": "your_email@example.com",
    "Password": "your_email_password",
    "EnableSsl": true,
    "From": "your_email@example.com"
  },

  "S3Options": {
    "AccessKey": "your_access_key",
    "SecretKey": "your_secret_key",
    "BucketName": "your_bucket_name",
    "ServiceURL": "your_service_url"
  }
}
